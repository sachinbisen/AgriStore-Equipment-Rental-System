<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>AgriStore_Feedback_Rating_Range</fullName>
    <active>true</active>
    <description>Ensures feedback rating is between 1 and 5</description>
    <errorConditionFormula>AND(
    NOT(ISBLANK(Rating__c)),
    OR(Rating__c &lt; 1, Rating__c &gt; 5)
)</errorConditionFormula>
    <errorDisplayField>Rating__c</errorDisplayField>
    <errorMessage>Rating must be between 1 and 5.</errorMessage>
</ValidationRule>