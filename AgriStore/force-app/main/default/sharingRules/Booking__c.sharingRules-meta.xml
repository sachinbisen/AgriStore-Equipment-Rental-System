<?xml version="1.0" encoding="UTF-8"?>
<SharingRules xmlns="http://soap.sforce.com/2006/04/metadata">
    <sharingOwnerRules>
        <fullName>Booking_Equipment_Owner_Access</fullName>
        <accessLevel>Read</accessLevel>
        <description>Booking sharing rule for Equipment Owners to view related bookings</description>
        <label>Booking - Equipment Owner Access</label>
        <sharedTo>
            <role>AgriStore_Equipment_Owner</role>
        </sharedTo>
        <sharedFrom>
            <roleAndSubordinates>AgriStore_Farmer</roleAndSubordinates>
        </sharedFrom>
    </sharingOwnerRules>
    <sharingOwnerRules>
        <fullName>Booking_Rental_Manager_Access</fullName>
        <accessLevel>Edit</accessLevel>
        <description>Booking sharing rule for Rental Managers to manage all bookings</description>
        <label>Booking - Rental Manager Access</label>
        <sharedTo>
            <role>AgriStore_Rental_Manager</role>
        </sharedTo>
        <sharedFrom>
            <roleAndSubordinates>AgriStore_Farmer</roleAndSubordinates>
        </sharedFrom>
    </sharingOwnerRules>
    <sharingOwnerRules>
        <fullName>Booking_Higher_Manager_Access</fullName>
        <accessLevel>Edit</accessLevel>
        <description>Booking sharing rule for Higher Managers to have full access</description>
        <label>Booking - Higher Manager Access</label>
        <sharedTo>
            <role>AgriStore_Higher_Manager</role>
        </sharedTo>
        <sharedFrom>
            <roleAndSubordinatesInternal>AgriStore_Farmer</roleAndSubordinatesInternal>
        </sharedFrom>
    </sharingOwnerRules>
</SharingRules>